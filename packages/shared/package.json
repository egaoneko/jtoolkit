{
  "name": "@toolkit/shared",
  "version": "0.0.1",
  "description": "shared",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "types": "tsc --emitDeclarationOnly",
    "prebuild": "yarn run clean && yarn run types",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix",
    "lint-style": "stylelint 'src/**/*.scss' --allow-empty-input --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "preschema": "rimraf ./src/schemas",
    "schema": "node scripts/schema.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/builder-webpack5": "6.4.9",
    "@storybook/manager-webpack5": "6.5.0-alpha.5",
    "@storybook/react": "6.4.9",
    "@toolkit/config": "0.0.1",
    "@toolkit/schema": "0.0.1",
    "@types/react-is": "17.0.3",
    "storybook-dark-mode": "1.0.8"
  }
}
